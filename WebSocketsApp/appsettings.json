{
  "SwaggerConfiguration": {
    "ApplicationName": "Web Sockets Api",
    "Path": "webSocketApi",
    "Version": "v1",
    "Title": "Api para Web Sockets (Hub)",
    "Description": "Documentación del WebSocket para la aplicación móvil de Tickets Ecuador",
    "DocumentsXml": [
      "WebSocketsApp.xml"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "WebSocketsApp.Hubs":"Debug",
        "Common.WebHub.ControllerHub.ConcurrentHub": "Debug",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Common.WebApi.Authorization.TokenAuthenticationHandler": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "WebSocketsApi"
    }
  },
  "JwtSettings": [
    {
      "Identifier": "Mobile",
      "Issuer": "Tickets_Ecuador",
      "Audience": "clients",
      "AccessExpiration": 30
    }
  ],
  "CustomConnectionStrings": [
    {
      "Identifier": "ApiCore",
      "CommandTimeOut": 15,
      "EnableSensitiveDataLogging": true,
      "EnableDetailedErrors": true
    }
  ],
  "BlobConfiguration": {
    "CurrentImplementation": "AzureStorage",
    "Implementations": [
      {
        "Identifier": "AzureStorage",
        "Information": {}
      }
    ]
  },
  "QueueConfiguration": {
    "CurrentImplementation": "Azure",
    "Implementations": [
      {
        "Identifier": "Azure",
        "Information": {
        }
      }
    ]
  },
  "SignalRConfiguration": {},
  "Cors": {
    "NamePolicy": "default",
    "Origins": [
      "*"
    ]
  },
  "RsaSecurityConfiguration": {
    "Implementations": [
      {
        "Identifier": "ServerGeneral",
        "Information": {
          "PrivateKeyBase64": "",
          "PublicKeyBase64": ""
        }
      },
      {
        "Identifier": "DeviceGeneral",
        "Information": {
          "PrivateKeyBase64": "",
          "PublicKeyBase64": ""
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "TimeZone": "America/Guayaquil",
  "CacheConfiguration": [
    {
      "Identifier": "Dotnet"
    }
  ]
}