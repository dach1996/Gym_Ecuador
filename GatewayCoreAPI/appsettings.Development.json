{
  "LogSensitiveInformation": true,
  "LogHeadersRemove": [
    "Authorization",
    "X-Secret",
    "X-FingerPrint",
    "X-Content",
    "X-SwitchImplementation",
    "X-Onboarding",
    "X-Channel",
    "Connection",
    "Accept-Encoding",
    "Host",
    "Accept",
    "user-agent",
    "MAX-FORWARDS",
    "X-ARR-LOG-ID",
    "DISGUISED-HOST",
    "X-SITE-DEPLOYMENT-ID",
    "WAS-DEFAULT-HOSTNAME",
    "X-FORWARDED-PROTO",
    "X-APPSERVICE-PROTO",
    "X-ARR-SSL",
    "X-FORWARDED-TLSVERSION",
    "X-FORWARDED-FOR",
    "X-ORIGINAL-URL",
    "X-WAWS-UNENCODED-URL",
    "X-TIMEZONE"
  ],
  "Serilog": {
    "WriteTo": [
      {
        "Name": "ApplicationInsights",
        "Args": {
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ]
  },
  "AuthenticationServiceConfiguration": {
    "CurrentImplementation": "Google"
  }
}