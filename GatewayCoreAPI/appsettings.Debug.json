{
  "LogSensitiveInformation": true,
  "LogHeadersRemove": [
    "Authorization",
    "X-Secret",
    "X-FingerPrint",
    "X-Content",
    "X-SwitchImplementation",
    "X-Onboarding",
    "X-Channel",
    "Connection",
    "Accept-Encoding",
    "Host",
    "Accept",
    "user-agent",
    "CONTENT-LENGTH",
    "CONTENT-TYPE",
    "X-REQUESTID",
    "X-TIME",
    "X-LANGUAGE",
    "X-DYNATRACE-APPLICATION",
    "X-DYNATRACE-REQUESTSTATE",
    "TRACEPARENT",
    "X-DYNATRACE",
    "TRACESTATE",
    "X-TIMEZONE"
  ],
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] ({RequestId}) {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341/"
        }
      }
    ]
  }
}