{
  "SwaggerConfiguration": {
    "ApplicationName": "Gateway Api Core",
    "Path": "apicore",
    "Version": "v1",
    "Title": "Pasajes Digitales Api",
    "Description": "Documentación del Gateway para la aplicación móvil de Tickets Ecuador",
    "DocumentsXml": [
      "GatewayCoreAPI.xml",
      "LogicApi.Model.xml"
    ]
  },
  "Serilog": {
    "Using": [
      "Serilog.Enrichers.ClientInfo"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http.HttpClient.CustomSignalR.LogicalHandler":"Warning",
        "System.Net.Http.HttpClient.CustomSignalR.ClientHandler":"Warning",
        "Microsoft.EntityFrameworkCore": "Information",
        "LogicApi.BusinessLogic.TicketHandler.GetTicketsAvailableHandler": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "ApiCore"
    }
  },
  "JwtSettings": [
    {
      "Identifier": "Mobile",
      "Issuer": "Tickets_Ecuador",
      "Audience": "clients",
      "AccessExpiration": 30
    }
  ],
  "CustomConnectionStrings": [
    {
      "Identifier": "ApiCore",
      "CommandTimeOut": 15,
      "EnableSensitiveDataLogging": true,
      "EnableDetailedErrors": true
    }
  ],
  "BlobConfiguration": {
    "CurrentImplementation": "AzureStorage",
    "Implementations": [
      {
        "Identifier": "AzureStorage",
