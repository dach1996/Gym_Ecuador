  "RsaSecurityConfiguration": {
    "Implementations": [
      {
        "Identifier": "ServerGeneral",
        "Information": {
          "PrivateKeyBase64": "",
          "PublicKeyBase64": ""
        }
      },
      {
        "Identifier": "DeviceGeneral",
        "Information": {
          "PrivateKeyBase64": "",
          "PublicKeyBase64": ""
        }
      }
